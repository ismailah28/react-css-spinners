    "css",
    "spinners"
  ],
  "author": "",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ismailah28/react-css-spinners/issues"
  },
  "homepage": "https://github.com/ismailah28/react-css-spinners#readme",
  "devDependencies": {
    "eslint": "^6.4.0",
    "eslint-config-standard": "^14.1.0",
    "eslint-plugin-import": "^2.20.1",
    "eslint-plugin-node": "^11.0.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.1",
    "husky": "^4.2.3",
    "lint-staged": "^10.0.7",
    "prettier-eslint": "^9.0.1",
    "prettier-eslint-cli": "^5.0.0"
  }
